{
  "name": "natours",
  "version": "1.0.0",
  "description": "Simple page to estudy html and scss",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.css",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "server": "live-server --port=9090",
    "build": "npm run compile:sass",
    "start": "concurrently \"npm run watch:sass\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegolopes98/natours.git"
  },
  "keywords": [
    "scss",
    "natours",
    "npm",
    "css",
    "html",
    "frontend"
  ],
  "author": "Diego",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diegolopes98/natours/issues"
  },
  "homepage": "https://github.com/diegolopes98/natours#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1"
  }
}
